{
    "name": "craigfowler/plugin-meta",
    "description": "A simple meta package for my commonly used WordPress plugins",
    "type": "wordpress-muplugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Fowler",
            "email": "craig@howler.studio"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "require": {
        "log1x/modern-acf-options": "dev-master",
        "log1x/modern-login": "dev-master",
        "wpackagist-plugin/duplicate-page": "^4.3",
        "wpackagist-plugin/enable-media-replace": "^4.0",
        "wpackagist-plugin/post-type-switcher": "^3.2",
        "wpackagist-plugin/safe-svg": "^2.1",
        "wpackagist-plugin/simple-history": "^4.1",
        "wpackagist-plugin/two-factor": "^0.9.1",
        "wpackagist-plugin/user-switching": "^1.5",
        "wpackagist-plugin/advanced-database-cleaner":"*",
        "wpackagist-plugin/redirection":"dev-trunk",
        "wpackagist-plugin/autodescription":"dev-trunk",
        "wpackagist-plugin/simple-page-ordering":"dev-trunk",
        "wpackagist-plugin/classic-editor":"dev-trunk",
        "wpackagist-plugin/classic-widgets":"dev-trunk"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
